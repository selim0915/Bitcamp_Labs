import java.util.ArrayList;

class EmpData{
	private ArrayList alist;
	private int[] numbers;
	
	EmpData(){
		this.alist = new ArrayList();
		this.numbers = new int[10];
	}
	
	//alist >> getter(read return), setter(write)
	public ArrayList getAlist() {
		return alist;
	}
	
	public void setAlist() {
		this.alist = alist;
	}
	
}


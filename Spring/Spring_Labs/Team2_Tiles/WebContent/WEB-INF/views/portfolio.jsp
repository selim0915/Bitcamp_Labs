<form action="${pageContext.request.contextPath}/index.htm">
	<div id="fh5co-main">
		<div id="fh5co-hero" class="js-fullheight"
			style="font-size: 30px; font-family: Roboto">
			<div class="fh5co-narrow-content animate-box">
				<div class="col-md-12"
					style="text-align: center; margin-bottom: 50px">SignUp</div>

				<div class="col-md-3"
					style="text-align: right; font-size: 25px; padding: 8px">Name</div>
				<div class="col-md-7">
					<div class="form-group">
						<input type="text" class="form-control" placeholder="Name">
					</div>
				</div>

				<div class="col-md-3"
					style="text-align: right; font-size: 25px; padding: 8px; margin-bottom: 10px">ID</div>
				<div class="col-md-7">
					<div class="form-group">
						<input type="text" class="form-control" placeholder="User ID">
					</div>
				</div>

				<div class="col-md-3"
					style="text-align: right; font-size: 25px; padding: 8px">Password</div>
				<div class="col-md-7">
					<div class="form-group">
						<input type="text" class="form-control" placeholder="Password">
					</div>
				</div>
				<div class="col-md-3"
					style="text-align: right; font-size: 25px; padding: 8px">Phone</div>
				<div class="col-md-7">
					<div class="form-group">
						<input type="text" class="form-control" placeholder="PhoneNum">
					</div>
				</div>
				<div class="col-md-3"
					style="text-align: right; font-size: 25px; padding: 8px">Address</div>
				<div class="col-md-7">
					<div class="form-group">
						<input type="text" class="form-control" placeholder="Address">
					</div>
					<div class="col-md-10" style="text-align: center">
						<input type="submit" value="Submit" class="btn btn-primary btn-md">
					</div>
				</div>

			</div>
		</div>
	</div>
</form>